<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Base application theme. -->
    <style name="Module_WanAndroid_AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/module_wanandroid_colorPrimary</item>
        <item name="colorPrimaryDark">@color/module_wanandroid_colorPrimaryDark</item>
        <item name="colorAccent">@color/module_wanandroid_colorAccent</item>
    </style>

    <style name="Module_WanAndroid_AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Module_WanAndroid_AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Module_WanAndroid_AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>